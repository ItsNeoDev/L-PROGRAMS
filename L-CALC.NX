PRINT "WELCOME TO L-CALC!"
PRINT "(USE + - * / OR ^)"
DO
INPUT "WHAT CALCULATION DO YOU WANT? ";L$
IF L$="+" THEN PRINT "ADDITON"
IF L$="-" THEN PRINT "SUBTRACTION"
IF L$="*" THEN PRINT "MULTIPLICATION"
IF L$="/" THEN PRINT "DIVISION"
IF L$="^" THEN PRINT "EXPONENT"
INPUT "NUMBER A:";A
INPUT "NUMBER B:";B
IF L$="+" THEN PRINT "SUM:";A+B
IF L$="-" THEN PRINT "SUM:";A-B
IF L$="*" THEN PRINT "SUM:";A*B
IF L$="/" THEN PRINT "SUM:";A/B
IF L$="^" THEN PRINT "SUM:";A^B
LOOP